<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>@ViewData["Title"] - PortfolioApp</title>
    <meta name="description" content="">
    <meta name="keywords" content="">
    @* <partial name="_StylesPartial" /> *@
    @await Html.PartialAsync("_StylesPartial")
    @RenderSection("StylesSection", false)
</head>

<body class="index-page">
    @await Html.PartialAsync("_HeaderPartial")
    
    <main class="main">
        @RenderBody()
    </main>
    @await Html.PartialAsync("_FooterPartial")
    @await Html.PartialAsync("_ScriptsPartial")
    @RenderSection("ScriptsSection", false)
</body>

</html>